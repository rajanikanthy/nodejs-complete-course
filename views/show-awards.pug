extends layout

block content
  h1= title
  .card(style='width: 18rem;')
    .card-body
      ul.list-group.list-group-flush
        each award in awards
          li.list-group-item=`${award.season} - ${award.type}`
        a(href=`/players/${player.id}/addAwardView` class='btn btn-primary') Add New Award